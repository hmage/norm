#!/bin/bash

fetch_source https://www.nasm.us/pub/nasm/releasebuilds/3.01/nasm-3.01.tar.xz f0531dfe9728192fe190cc4932df44a26f1bde7c

do_unpack
## Internal md5.h is more important than libmd's md5.h
sed -i.bak 's|$(BUILD_CFLAGS) $(INTERNAL_CFLAGS)|$(INTERNAL_CFLAGS) $(BUILD_CFLAGS)|' "$SRCDIR/Makefile.in"
do_compile
