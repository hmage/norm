#!/bin/bash

if ! test_cc_value asm/bitsperlong.h __BITS_PER_LONG; then
    log "libseccomp requires linux kernel headers that have asm/bitsperlong.h"
    false
fi

fetch_source https://github.com/seccomp/libseccomp/releases/download/v2.4.3/libseccomp-2.4.3.tar.gz 477a66a6c5a32e585adaf90961994641de313247

do_undebian_compile
