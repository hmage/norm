#!/bin/bash

depends_on pkg-config
depends_on autoconf automake libtool ## for autoreconf
depends_on libjpeg   ## lossy DNG files
depends_on libjasper ## lossy DNG files

fetch_source http://www.libraw.org/data/LibRaw-0.22.0.tar.gz ad2eee2e12714ef7b86733690931b0edc085294a

do_unpack
do_autoreconf
do_compile
