#!/bin/bash

depends_on liblmdb
depends_on libgnutls
depends_on liburcu
[[ $OSTYPE != *darwin* ]] && depends_on libedit ## on osx it's always there, so no need to build it

fetch_source https://secure.nic.cz/files/knot-dns/knot-3.4.8.tar.xz 8b0f8c2839d609515143aa33b50d0d2773155f45

do_unpack_compile
