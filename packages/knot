#!/bin/bash

depends_on liblmdb
depends_on libgnutls
depends_on liburcu
[[ $OSTYPE != *darwin* ]] && depends_on libedit ## on osx it's always there, so no need to build it

fetch_source https://secure.nic.cz/files/knot-dns/knot-3.5.3.tar.xz 390b3c3284c8daedb09381520db7d2a6f26cad31

do_unpack_compile
