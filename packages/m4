#!/bin/bash

## using debian patch to fix build failure on glibc 2.28 and newer
fetch_source http://ftpmirror.gnu.org/m4/m4-1.4.18.tar.gz 2f76f8105a45b05c8cfede97b3193cd88b31c657
fetch_debian http://deb.debian.org/debian/pool/main/m/m4/m4_1.4.18-2.debian.tar.xz d1ffe7863857b76f55e5ef6b4ddb2d18c5944a99

do_undebian_compile
