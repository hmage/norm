#!/bin/bash

depends_on zstd
needs_tool cmake

fetch_source https://github.com/ccache/ccache/releases/download/v4.7.4/ccache-4.7.4.tar.xz fc5c1c0f7dac2db128375e1b24be5f96f29e6f31

do_unpack_compile -DREDIS_STORAGE_BACKEND=OFF -DENABLE_IPO=ON

add_to_ccache cc cpp gcov c++ g++ gcc gcc-ar gcc-nm gcc-ranlib
