#!/bin/bash

depends_on zstd
needs_tool cmake

fetch_source https://github.com/ccache/ccache/releases/download/v4.7.3/ccache-4.7.3.tar.xz 71295341d99b9254a80e7dd30bf03c322859c268

do_unpack_compile -DREDIS_STORAGE_BACKEND=OFF -DENABLE_IPO=ON

add_to_ccache cc cpp gcov c++ g++ gcc gcc-ar gcc-nm gcc-ranlib
