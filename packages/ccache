#!/bin/bash

depends_on zstd
needs_tool cmake

fetch_source https://github.com/ccache/ccache/releases/download/v4.12.2/ccache-4.12.2.tar.xz 475c2f46997071f5dad2f4fc3329971b6ed73f31

do_unpack_compile -DREDIS_STORAGE_BACKEND=OFF -DENABLE_IPO=ON -DSTATIC_LINK=ON

add_to_ccache cc cpp gcov c++ g++ gcc gcc-ar gcc-nm gcc-ranlib
