#!/bin/bash

depends_on zstd
needs_tool cmake

fetch_source https://github.com/ccache/ccache/releases/download/v4.6.1/ccache-4.6.1.tar.xz dd6837f9153cac54cc61335d21035dcdcd721512

do_unpack_compile -DREDIS_STORAGE_BACKEND=OFF -DENABLE_IPO=ON

add_to_ccache cc cpp gcov c++ g++ gcc gcc-ar gcc-nm gcc-ranlib
