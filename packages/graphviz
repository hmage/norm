#!/bin/bash

depends_on autoconf automake libtool
depends_on bison ## system-provided one does not understand modern flags

fetch_source https://gitlab.com/graphviz/graphviz/-/archive/6.0.1/graphviz-6.0.1.tar.bz2 c3ae94bc38326cb0b4c1fbd189a581520b4c0a9f

CONFOPTS=(
)

do_unpack
pushd_src
./autogen.sh
popd_src
do_compile "${CONFOPTS[@]}"
