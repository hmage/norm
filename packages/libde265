#!/bin/bash

fetch_source https://github.com/strukturag/libde265/releases/download/v1.0.3/libde265-1.0.3.tar.gz 3a246d4c526422c85443edcd8bc904004b2b42c7

CONFFLAGS=(
--disable-sherlock265 ## sherlock265 is a GUI tool to analyze h265 frames, but requires QT to build
)

do_unpack_compile "${CONFFLAGS[@]}"
